<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/more_notice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/notice"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="165dp"/>
    <ImageView
        android:id="@+id/more_inquire"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/inquire"
        app:layout_constraintTop_toBottomOf="@id/more_notice"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/more_change_theme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/change_theme"
        app:layout_constraintTop_toBottomOf="@id/more_inquire"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/more_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/logout"
        app:layout_constraintTop_toBottomOf="@id/more_change_theme"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/more_withdraw_membership"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/withdraw_membership"
        app:layout_constraintTop_toBottomOf="@id/more_logout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"/>
    <ImageView
        android:id="@+id/more_app_version"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/app_version"
        app:layout_constraintTop_toBottomOf="@id/more_withdraw_membership"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>